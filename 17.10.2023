Option Compare Database

Private Sub Polecenie0_Click()
 Dim bd As DAO.Database
 Dim rs As DAO.Recordset
 Dim odp As Integer
 
 Set bd = CurrentDb
 Set rs = bd.OpenRecordset("Klienci")
 
 rs.MoveFirst
 
 Do While Not rs.EOF
   odp = MsgBox("Nazwisko klienta : " & rs.Fields("NazwiskoKlienta") & vbCrLf, vbOKCancel, "Szulta modu≈Ç VBA")
   If odp = vbCancel Then
     Exit Sub
   End If
   rs.MoveNext
   
 Loop
 
End Sub
